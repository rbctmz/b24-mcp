# Bitrix24 REST endpoint without trailing slash.
BITRIX_BASE_URL=https://example.bitrix24.ru/rest

# OAuth token or webhook key.
BITRIX_TOKEN=example_token

# Optional instance name for metadata.
# BITRIX_INSTANCE_NAME=demo

# HTTP timeout in seconds (optional).
# BITRIX_TIMEOUT_SECONDS=10

# FastAPI/uvicorn server settings.
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
SERVER_LOG_LEVEL=info
